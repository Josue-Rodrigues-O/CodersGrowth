<mvc:View
    controllerName="controle.funcionarios.Controller.Detalhes"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    height="100%">
    <Page
        title="{i18n>tituloPaginaDetalhes}"
        showNavButton="true"
        navButtonPress=".aoClicarVoltarParaPaginaAnterior">
        <ObjectHeader
            title="{modeloTabelaFuncionarios>nome}" />
        <HBox>
            <HBox><ObjectIdentifier title="CPF" /></HBox>
            <HBox><ObjectIdentifier title="Telefone" /></HBox>
            <HBox><ObjectIdentifier title="salário" /></HBox>
            <HBox><ObjectIdentifier title="Casado(a)?" /></HBox>
            <HBox><ObjectIdentifier title="Gênero" /></HBox>
            <HBox><ObjectIdentifier title="Data de Nascimento" /></HBox>
        </HBox>
    </Page>
</mvc:View>